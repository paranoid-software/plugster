      "^.+\\.mjs$": "babel-jest"
    },
    "testRegex": "((\\.|/*.)(spec))\\.js?$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://pablo-pinargote@github.com/paranoid-software/plugster.git"
  },
